import groovy.transform.Field
@Field def myvar = "ACCOUNT_ID"

def myprint() {
    sh "echo ${myvar}"
}

pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        script { myprint() }
      }
    }
    stage('is master') {
      when {
        branch 'master'
      }
      steps {
        println 'is master'
      }
    }
    stage('is develop') {
      when {
        branch 'develop'
      }
      steps {
        println 'is develop'
      }
    }
  }
  post {
    success {
      println 'success'
    }
    aborted {
      println 'aborted'
    }
    failure {
      println 'failure'
    }
  }
}
